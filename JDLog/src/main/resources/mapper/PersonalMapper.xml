<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jidouauto.log.dao.PersonalDao" >
    <sql id="BASE_TABLE">
        person
    </sql>

    <sql id="BASE_COLUMN">
        id,name,age
    </sql>

    <select id="getAll" resultType="com.jidouauto.log.model.PersonalEntity">
        SELECT
        <include refid="BASE_COLUMN"/>
        FROM
        <include refid="BASE_TABLE"/>
    </select>

</mapper>